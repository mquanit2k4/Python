    "from math import*\n",
    "# Dung ham co san trong thu vien math\n",
    "def C(k,n):\n",
    "    return comb(n,k)\n",
    "\n",
    "# Dung cong thuc\n",
    "def Ce(k,n):\n",
    "    return factorial(n)//(factorial(k)*factorial(n-k))\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    k , n = map(int, input().split())\n",
    "    print(Ce(k,n))\n"
