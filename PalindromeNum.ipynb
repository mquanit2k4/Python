    "def reverseNum(n):\n",
    "    res = 0\n",
    "    tmp = n\n",
    "    while n != 0:\n",
    "        res =  res*10 + n % 10\n",
    "        n = n // 10\n",
    "    return res == tmp\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    print(reverseNum(n))"
   ]
  }
